{
  "name": "media-server",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "tsc && vite build && PORT=3013 node ./server.js",
    "vite": "vite --host",
    "serve": "node ./server.js",
    "dev": "nodemon --legacy-watch --ignore 'src/*' --ignore 'dist/*' ./server.js",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "watch": "vite build --watch",
    "prepare": "husky install",
    "debuffs": "node ./utils/update-debuff-list.js",
    "addons": "node ./update-addons.js",
    "force-addons": "node ./update-addons.js force",
    "test": "node ./test.js"
  },
  "dependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "localforage": "^1.10.0",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "jsdom": "^20.0.3",
    "nodemon": "^2.0.21",
    "prettier": "2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "pretty-quick": "^3.1.3"
  }
}
